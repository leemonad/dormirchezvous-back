{
  "name": "dormirchezvous",
  "version": "0.0.2",
  "private": true,
  "description": "Projet de mise en relation",
  "main": "src/server.js",
  "scripts": {
    "setup-backend": "node -r dotenv/config -r babel-register ./src/api/setup.js",
    "build-backend": "babel -d ./dist/server ./src/api",
    "start-backend": "npm run setup-backend && nodemon --exec \"node -r dotenv/config -r babel-register src/index.js\"",
    "build-frontend": "NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "start-frontend": "webpack",
    "format": "prettier {src,config}/{*.js,**/*.js} --write --trailing-comma all --single-quote"
  },
  "dependencies": {
    "admin-on-rest": "^1.3.2",
    "aor-simple-graphql-client": "0.0.11",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-client-preset": "^1.0.5",
    "apollo-link-http": "^1.3.1",
    "async": "^2.5.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "casual": "^1.5.17",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.6",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.0",
    "graphql-tools": "^2.12.0",
    "html-loader": "^0.5.1",
